                                    <GradientStop Offset="1" Color="#3aa0ff" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <Border.Clip>
                                <RectangleGeometry
                                    RadiusX="10"
                                    RadiusY="10"
                                    Rect="0, 0, 350, 200" />
                            </Border.Clip>
