        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="200" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="75" />
                <RowDefinition />
            </Grid.RowDefinitions>

            <Border
                Grid.Column="1"
                Width="180"
                Height="50"
                Margin="40,0,0,0"
                HorizontalAlignment="Left"
                Background="#333540"
                BorderThickness="0"
                CornerRadius="10">
                <Button
                    Width="180"
                    Height="50"
                    HorizontalAlignment="Left"
                    Background="Transparent"
                    BorderThickness="0"
                    Click="GetData"
                    Content="Set Paramenters"
                    FontFamily="/Fonts/#SAO UI TT"
                    FontSize="26"
                    Foreground="White" />
            </Border>


            <TextBlock
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                FontFamily="/Fonts/#SAO UI TT"
                FontSize="28"
                Foreground="White"
                Text="Save Modder" />
