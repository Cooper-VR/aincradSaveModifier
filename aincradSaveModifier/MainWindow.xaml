                <RadioButton
                    Name="HomeButton"
                    Height="50"
                    Click="HomePressed"
                    Content="Home"
                    FontFamily="/Fonts/#SAO UI TT"
                    FontSize="24"
                    Foreground="White"
                    IsChecked="True"
                    Style="{StaticResource MenuButtonTheme}" />
                <RadioButton
                    Name="StatsButton"
                    Height="50"
                    Click="StatsPressed"
                    Content="Stats"
                    FontFamily="/Fonts/#SAO UI TT"
                    FontSize="24"
                    Foreground="White"
                    Style="{StaticResource MenuButtonTheme}" />
                <RadioButton
                    Name="InventoryButton"
                    Height="50"
                    Click="InventoryPressed"
                    Content="Inventory"
                    FontFamily="/Fonts/#SAO UI TT"
                    FontSize="24"
                    Foreground="White"
                    Style="{StaticResource MenuButtonTheme}" />

            </StackPanel>
