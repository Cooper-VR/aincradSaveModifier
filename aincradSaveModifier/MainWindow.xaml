    xmlns:viewModel="clr-namespace:aincradSaveModifier.MVVM.ViewModel"
    Title="MainWindow"
    Width="920"
    Height="600"
    Background="#272537"
    ResizeMode="NoResize"
    mc:Ignorable="d">
    <Window.DataContext>
        <viewModel:MainViewModel />
    </Window.DataContext>
